{
  "name": "docketeer",
  "version": "6.0.0",
  "license": "MIT",
  "description": "A Docker Visualizer",
  "author": "Team Docketeer",
  "proxy": "http://localhost:3000",
  "main": "./dist/Docketeer.js",
  "scripts": {
    "electron": "electron .",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dist": "npm run compile && electron-builder",
    "dist-win": "npm run compile && electron-builder --win --x64",
    "dist:dir": "yarn dist  --dir -c.compression=store -c.mac.identity=null",
    "test": "jest --verbose",
    "temp": "webpack-dev-server --open --mode development"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/react-router-dom": "^5.3.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.3",
    "clsx": "^1.1.1",
    "cors": "^2.8.5",
    "electron-fetch": "^1.7.3",
    "express": "^4.17.1",
    "fix-path": "^3.0.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.2",
    "parse": "^3.3.0",
    "pg": "^8.7.1",
    "qs": "^6.7.0",
    "react": "^16.14.0",
    "react-chartjs-2": "^2.10.0",
    "react-modal": "^3.14.2",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.2",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.2.1",
    "twilio": "^3.50.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "babel-loader": "^8.2.5",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "electron": "^21.0.1",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sass-loader": "^10.2.0",
    "typescript": "^3.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/docketeer.git"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "build": {
    "extraResources": [
      {
        "from": "./src/database/",
        "to": "database",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
