{
  "name": "docketeer",
  "version": "6.0.0",
  "license": "MIT",
  "description": "A Docker Visualizer",
  "author": "Team Docketeer",
  "main": "dist/DockteerElectron.js",
  "scripts": {
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development\" \"nodemon server/server.js\" \"npm run dev:electron\"",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development\" \"nodemon server/server.js\" \"npm run dev:electron\"",
    "buildWeb": "webpack",
    "dist": "npm run compile && electron-builder",
    "dist-win": "npm run compile && electron-builder --win --x64",
    "dist:dir": "yarn dist  --dir -c.compression=store -c.mac.identity=null",
    "test": "jest --verbose"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.8",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "colors": "^1.4.0",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0",
    "react-error-overlay": "^6.0.11",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "react-scripts": "^5.0.1",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@redux-devtools/core": "^3.13.1",
    "@types/pg": "^8.6.5",
    "axios": "^1.0.0",
    "babel-loader": "^8.2.5",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.3",
    "electron": "^21.0.1",
    "electron-devtools-installer": "^3.2.0",
    "express": "^4.18.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.1.2",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "react-devtools": "^4.26.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "twilio": "^3.82.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/docketeer.git"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
